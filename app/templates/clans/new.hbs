<form>
	<div class="form-group">
	<h2>Add New Clan:</h2>
		<label>Tag</label>
		{{input type="text" class="form-control" value=tag placeholder="Add Clan Tag..."}}
	</div>
	<div class="form-group">
		<label>Date Added</label>
		{{input type="date" class="form-control" value=date}}
	</div>
	<div class="form-group">
		<label>Who Added</label>
		{{textarea class="form-control" value=adderName placeholder="Your Name..."}}
	</div>
	<button {{action 'addClan'}} class="btn btn-primary">Submit</button> 
</form>